{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "semi": "error",
    "no-undef": "off",
    "no-unused-vars": "warn",
    "no-control-regex": "off",
    "no-unused-expressions": "off",
    "react/prop-types": "off",
    "no-class-assign": "off",
    "no-unescaped-entities": "off",
    "react/display-name": "off"
  }
}